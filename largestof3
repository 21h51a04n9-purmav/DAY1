def display(data):
    print(f"largest number is:{data}")
def get_input():
    a=int(input("enter a value:"))
    b=int(input("enter b value:"))
    c=int(input("enter c value:"))
    if(a>b and a>c):
        print("largest=a",a)
    elif(b>a and b>c):
        print("largest=b",b)
    else:
        print("largest=c",c)
def main():
    get_input()
main()
