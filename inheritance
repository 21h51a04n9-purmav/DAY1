class Parent:
   def __init__(self):
      self.bigNose="3CM" 
   def dispaly_parent(self):
      print("This is the parent class.")
class Child(Parent):
   def __init__(self):
      super().__init__()
   def display_child(self):
      print("This is the child class.")
child=Child()
child.dispaly_parent()
child.display_child()
print(child.bigNose)
p=Parent()
ch=Child()
p=ch
